///$tab 2 - CASOS DISTRIBUIDOS
CASOS_DISTRIBUIDOS:
NoConcatenate
LOAD
Distinct
    %ID_PROCESSO,
    %ID_ORGAO_JULGADOR,
    'CASOS_NOVOS_DT_AUTUACAO'               AS FLAG_DISTRIBUIDOS,
    [Data Distribuicao],
    ID_USUARIO
Resident CABECALHO;

STORE CASOS_DISTRIBUIDOS INTO $(DirQVD)/Projetos/04_Painel_PJE/2.Transformação/CASOS_DISTRIBUIDOS.QVD (qvd);
DROP TABLE CASOS_DISTRIBUIDOS;